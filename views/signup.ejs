<%- include("partials/header") -%>
    <div class="w-25 mx-auto mt-5">
        <form action="/signup" method="POST">
            <div class="mb-3 text-center">
                <label for="ss" class="form-label text-center">User Name</label>
                <input type="text" class="form-control" id="ss" aria-describedby="emailHelp" name="username" required>
            </div>
            <div class="mb-3 text-center">
                <label for="dd" class="form-label">Password</label>
                <input type="password" class="form-control" id="dd" name="userpassword" required>
            </div>
            <div class="text-center">
                <button type="submit" class="btn btn-outline-success ">Sign Up</button>
            </div>
        </form>
    </div>
    <hr>
    <div class="text-center">
        <a href="/login">Login</a>

    </div>
    <%- include("partials/header") -%>
